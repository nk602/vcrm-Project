<%- include('includes/meta-head') %>
<body class="ltr main-body leftmenu dash_admin light-theme">

	<%- include('includes/header-loader') %>

	<!-- Page -->
	<div class="page">

		<!-- Main Header-->
		<%- include('includes/header') %>
		<!-- End Main Header-->

		<!-- Main Content-->
		<div class="main-content side-content pt-0">

			<div class="main-container container-fluid">
				<div class="inner-body">

					<!-- Page Header -->
					<div class="page-header">
						<div>
							<h2 class="main-content-title tx-24 mg-b-5">Staff List</h2>
							<ol class="breadcrumb">
								<li class="breadcrumb-item"><a href="#">HRMS </a></li>
								<li class="breadcrumb-item active" aria-current="page">Staff List </li>
							</ol>
						</div>
						<div class="d-flex">
							<div class="justify-content-center">
								<a href="/admin/dashboard/register-employee" type="button" class="btn btn-primary my-2 btn-icon-text"> <i
										class="fe fe-plus me-2"></i> Add Staff</a>
							</div>
						</div>
					</div>
					<!-- End Page Header -->

					<div class="row">
						<div class="col-lg-12">
							<div class="card custom-card">
								<div class="card-body py-3">
									<div class="row">
										<div class="col-sm-6">
											<input type="search" class="form-control form-control"
												placeholder="Search..." aria-controls="example1">
										</div>
										<div class="col-sm-2">
											<select class="form-control select2">
												<option label="Choose one">Gender</option>
												<option value="Firefox">Male</option>
												<option value="Chrome">Female</option>
											</select>
										</div>

										<div class="col-sm-2">
											<select class="form-control select2">
												<option label="Choose one">Department</option>
												<option value="Firefox"></option>
											</select>
										</div>

										<div class="col-sm-2">
											<select class="form-control select2">
												<option label="Choose one">Designation</option>
												<option value="Firefox"></option>
											</select>
										</div>

									</div>
								</div>
							</div>
						</div>
					</div>
					<!-- Row -->
					<div class="row row-sm">
						<div class="col-lg-12">
							<div class="card custom-card">
								<div class="card-body">

									<div class="table-responsive">
										<table class="table table-striped table-bordered text-nowrap mb-0">
											<thead>
												<tr>
													<th><label class="ckbox"><input type="checkbox"
																value="5"><span></span></label></th>
													<th>ID</th>
													<th>Photo</th>
													<th>Contact</th>
													<th>Department</th>
													<th>Lead Target(Monthly)</th>
													<th>KYC</th>
													<th>Actions</th>

												</tr>
											</thead>
											<tbody>
												<% employees.forEach(function(employee) { %>
												<tr>
													<td><label class="ckbox"><input type="checkbox"
																value="5"><span></span></label></td>
													<td>
														EMP-<%= employee.id %><br>
														DOJ-<%=employee.dateOfJoin %>
														<!--span class="plus_btn_" data-bs-target="#modaldemo-employee-form" data-bs-toggle="modal"></span-->
													</td>
													<td><img alt="avatar" class="rounded-circle me-3"
															src="../assets/img/users/5.jpg" style="width:60px"></td>
													<td>
														<p class="mb-0"><%= employee.firstName %> <%= employee.lastName %><br>
															M: <%= employee.mobileNumber %><br>
															E: <%= employee.email%></p>
													</td>
													<td>
														<p class="mb-0">Consulting<br>
															Sr. Associate
														</p>
													</td>
													<td>
														<p class="mb-0"> Projected Target : 300<br>
															Actual Target : 15</p>
													</td>
													<td>
														<p class="mb-0">Aadhaar: <a href="" class="mb-1" title="View"
																data-bs-target="#modaldemo-id" data-bs-toggle="modal"><i
																	class="fe fe-eye"></i></a><br>
															C.Cheque: <a href="" class=" mb-1" title="View"
																data-bs-target="#modaldemo-cheque"
																data-bs-toggle="modal"><i class="fe fe-eye"></i></a><br>
															Add Proof: <a href="" class=" mb-1" title="View"
																data-bs-target="#modaldemo-address"
																data-bs-toggle="modal"><i class="fe fe-eye"></i></a></p>
													</td>
													<td>
														<div class="main-toggle-group-demo">
															<div class="main-toggle main-toggle-success on">
																<span></span>
															</div>
														</div>
														<a href="staff-profile.html"
															class="btn ripple  btn-primary btn-xs w-100 mb-1 mt-1">View</a><br>
														<a href="staff-attendance.html"
															class="btn ripple btn-warning btn-xs w-100 mb-1">
															Attendance</a><br>
															<a href="/admin/dashboard/update-employee/<%= employee.id %>"onclick="return confirm('Are you sure you want to update <%= employee.firstName %>?')"
															class="btn ripple btn-info btn-xs w-100">Edit Profile</a>

															

												

												</tr>
												<% }); %>
											</tbody>
										</table>
									</div>
								</div>
							</div>
						</div>
					</div>
					<!-- End Row -->


				</div>
			</div>
		</div>
		<!-- End Main Content-->

		<!-- Main Footer-->
		<%- include('includes/footer') %>
		<!--End Footer-->


	</div>
	<!-- End Page -->


	<!-- Large Modal -->
	<div class="modal" id="modaldemo-id">
		<div class="modal-dialog modal-lg" role="document">
			<div class="modal-content modal-content-demo">
				<div class="modal-header">
					<h6 class="modal-title">Aadhaar</h6><button aria-label="Close" class="btn-close"
						data-bs-dismiss="modal" type="button"></button>
				</div>
				<div class="modal-body">
					<div class="row">
						<div class="col-sm-8">
							<img src="https://newadmin.webkype.com/assets/img/pngs/default-img.gif" style="width:100%;">
						</div>
						<div class="col-sm-4">
							<h6>Aadhaar : 234567837480</h6>
							<div class="form-group">
								<select class="form-control select2">
									<option>Status</option>
									<option>Approved</option>
									<option>Disapproved</option>
								</select>
							</div>
							<div class="form-group">
								<button class="btn ripple btn-primary w-100" type="button">Save changes</button>
							</div>
						</div>
					</div>


				</div>
			</div>
		</div>
	</div>
	<!--End Large Modal -->
