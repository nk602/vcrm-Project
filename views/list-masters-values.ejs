          .addEventListener("submit", async function (event) {
            event.preventDefault(); // Prevent the default form submission
            try {
              const form = event.target;
              const formData = new FormData(form); // Create a FormData object with the form data
            //  const masterId = formData.get("id");
              const response = await fetch(
                `/admin/dashboard/update-master-values/${formData.get("id")}`,
                {
                  method: "POST",
                  body: formData,
                }
              );

              const data = await response.json();

              if (data.status === "error") {
                throw new Error(data.message);
              }

              alert(data.message);
              setTimeout(() => {
                location.href = "/admin/dashboard/list-masters-values";
              }, 1500);
            } catch (error) {
              console.log(error);
              scrollTo(0, 0);
              document.querySelector(".errors").textContent = error.message;
            }
          });
