			event.preventDefault(); // Prevent the default form submission
			try {
			  const form = event.target;
			  const formData = new FormData(form); // Create a FormData object with the form data
			  const employeeId = formData.get("employeeId"); // Assuming there's an input with the name "employeeId" for identifying the employee
	  
			  const response = await fetch(`/admin/dashboard/update-employee/${employeeId}`, {
				method: "POST", // You can also use "PUT" depending on your API design
				body: formData,
			  });
	  
			  const data = await response.json();
	  
			  if (data.status === "error") {
				throw new Error(data.message);
			  }
	  
			  alert(data.message);
			  setTimeout(() => {
				location.href = "/admin/dashboard/employees";
			  }, 1500);
			} catch (error) {
			  console.log(error);
			  scrollTo(0, 0);
			  document.querySelector(".errors").textContent = error.message;
			}
		  });
